<h1>RespoSessions#index</h1>

<hr>
<%= form_for @respo, :url => { :action => 'sign_in_respo' }  do |f| %>
    <%= f.text_field :email, placeholder: "email" %>
    <%= f.password_field :password, placeholder: "password" %>

    <%= f.submit "Submit", disable_with: 'Submiting...', class: 'btn btn-outline-primary login-btn' %>
<% end %>